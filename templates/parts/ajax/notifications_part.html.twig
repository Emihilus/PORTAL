<h2 {% if notifications|length == 0 %}class="no-borders"{% endif %} onclick="window.location.href='{{path('my-notifications')}}';">{% if notifications|length == 0 %} Brak powiadomie≈Ñ {% else %}
	Powiadomienia - <span>{{ notifications|length }}</span>{% endif %}
</h2>

{% for notification in notifications %}
    

<div class="notifications-item">
	<div class="text">
		<h4>{{notification.createdAt|ago}}</h4>
		<p>{{notification.message}}</p>
	</div>
</div>

{% endfor %}
